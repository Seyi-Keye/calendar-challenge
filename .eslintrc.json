{
  "extends": "airbnb",
  "plugins": ["react", "jsx-a11y", "import"],
  "env": {
    "mocha": true,
    "browser": true
  },
  "globals": {
    "localStorage": true,
    "sinon": true,
    "$": true
  },
  "parser": "babel-eslint",
  "rules": {
    "arrow-parens": [0, "as-needed", { "requireForBlockBody": true }],
    "comma-dangle": 0
  }
}
